{
  "name": "@msrvida/sanddance-explorer-bundle",
  "version": "2.0.0",
  "description": "SandDance Explorer complete bundle for embedding.",
  "main": "dist/index.js",
  "browser": "dist/umd/sanddance-explorer-bundle.js",
