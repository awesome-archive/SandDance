    "office-ui-fabric-react": "6.204.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "vega": "^5.8"
  },
  "devDependencies": {
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@msrvida/office-ui-fabric-react-cdn-typings": "*"
